<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>join</servlet-name>
    <servlet-class>org.project.JoinRoomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>join</servlet-name>
    <url-pattern>/joinroom</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>create</servlet-name>
    <servlet-class>org.project.CreateRoomServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>create</servlet-name>
    <url-pattern>/createroom</url-pattern>
  </servlet-mapping>
</web-app>
